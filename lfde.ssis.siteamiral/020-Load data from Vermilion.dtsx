                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT 
       p.Code_ISIN AS IsinPart, 
       p.Code_Part AS CodePart, 
       f.Code_ISIN AS IsinFond, 
       f.Code_Fonds AS CodeFond, 
       f.PEA AS FondEligiblePEA, 
       f.Capitalisation_Moyenne, 
       f.Active_Share, 
       f.Nom_Fonds AS Nom_Fonds, 
       p.Nom_Part AS Nom_Part
FROM [dbo].[PART] p
     INNER JOIN [dbo].[FONDS] f ON p.Code_Fonds = f.Code_Fonds
                                   AND p.[Date] = f.[Date]
WHERE 1 = 1
      AND p.[Date] =
(
    SELECT MAX([Date])
    FROM part tp
    WHERE p.Code_Fonds = tp.Code_Fonds
);</property>
